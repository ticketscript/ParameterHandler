{
    "name": "ticketscript/composer-parameter-handler",
    "description": "Composer script handling your ignored parameter file",
    "keywords": ["parameters management"],
    "homepage": "https://github.com/ticketscript/composer-parameter-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Geoffrey de Kleijn",
            "email": "jay@jayhaabee.nl"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/yaml": "~2.0"
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev",
        "phpspec/prophecy-phpunit": "~1.0",
        "phpunit/phpunit": "~4.6",
        "symfony/filesystem": "~2.2"
    },
    "autoload": {
        "psr-4": { "Incenteev\\ParameterHandler\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
